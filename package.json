{
  "name": "harden",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.4.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-top-level-await": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "jest": "^26.6.1",
    "mime-types": "^2.1.27",
    "supertest": "^5.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "build:test": "webpack --config tests/webpack.config.js --mode=development",
    "watch": "webpack --watch --mode=development",
    "serve": "node -r dotenv/config dist/main.bundle.js",
    "test": "yarn build:test && jest dist/main.bundle.test.js"
  },
  "jest": {
    "setupFiles": ["dotenv/config"],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
